<div class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" data-warpjs-modal="{{modalName}}">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <div class="modal-title">ImageMap Editor</div>
      </div>
      <div class="modal-body">
        {{> loading}}
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" data-warpjs-action="save">Save <span class="glyphicon glyphicon-ok"></span></button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel <span class="glyphicon glyphicon-remove"></span></button>
      </div>
    </div>
  </div>
</div>
